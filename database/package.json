{
  "name": "database",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "knex:migrate:make": "knex --knexfile src/knexfile.ts migrate:make database_schema",
    "knex:migrate:latest": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "knex:seed:make": "yarn knex --knexfile src/knexfile.ts  seed:make",
    "knex:seed:run": "yarn knex --knexfile src/knexfile.ts seed:run"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "typescript": "^5.0.2"
  }
}
